{
  "name": "@opcjs/zoro-plugin",
  "version": "2.1.0",
  "description": "the plugin for zoro",
  "main": "index.js",
  "keywords": [
    "zoro",
    "plugin"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:cjs",
    "build:es": "babel src --out-dir ./ --copy-files",
    "build:cjs": "rollup --config rollup.config.cjs.js",
    "clean": "rm -rf ./extendModel.js ./index.js ./util.js ./loading.js ./mixin ./dist",
    "release": "npm run lint && npm run build && npm publish --access=public",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaureWu/zoro-plugin.git"
  },
  "author": "Faure Wu <502556093@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FaureWu/zoro-plugin/issues"
  },
  "homepage": "https://github.com/FaureWu/zoro-plugin",
  "directories": {
    "doc": "doc"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  }
}
