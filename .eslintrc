{
  "extends": [
    "standard",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "parser": "babylon",
        "semi": false
      }
    ]
  }
}
